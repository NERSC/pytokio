{
    "lmt_timestep": 5,
    "mount_to_fsname": {
        "^/projects/radix-io": "mira-fs1",
        "^/scratch1": "scratch1",
        "^/scratch2": "scratch2",
        "^/scratch3": "scratch3",
        "^/global/cscratch1": "cscratch",
        "^/var/opt/cray/dws/mounts/.*/ss": "bb-shared",
        "^/var/opt/cray/dws/mounts/.*/ps": "bb-private",
